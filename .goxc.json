{
	"AppName": "gotelemetry_agent",
	"Tasks": [
		"interpolate-source",
		"go-fmt",
		"default"
	],
	"BuildConstraints": "!plan9,!nacl,!solaris",
	"PackageVersion": "3.0.0",
	"ConfigVersion": "0.9",
	"TaskSettings": {
		"go-install": {
			"dir": "./..."
		},
		"go-fmt": {
			"dir": "./..."
		},
		"go-vet": {
			"dir": "./..."
		},
		"go-test": {
			"dir": "./..."
		},
		"publish-github": {
			"owner": "telemetryapp",
			"repository": "gotelemetry_agent",
			"prerelease": false,
			"body": "-  \n\n**Mac OS X Users Note:** The Agent for your platform will be the darwin_amd64 or darwin_386 releases."
		}
	}
}
